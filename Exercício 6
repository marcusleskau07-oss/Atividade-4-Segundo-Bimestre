programa
{
	
	funcao inicio()
	{
		inteiro n1, n2
		inteiro maior, menor
	
		escreva("Escreva um número: ")
		leia(n1)

		escreva("escreva um segundo número: ")
		leia(n2)

		se(n1>n2)
    {
			maior = n1
			menor = n2
		}
		senao{
			menor = n1
			maior = n2
		}

		escreva("\nNúmeros entre ", menor, " e ", maior, ":\n")

		para(inteiro n3 = menor+1; n3 < maior; n3++){
		escreva(n3, "\n")
		}
		
	}
}
