programa
{
	
	funcao inicio()
	{
		inteiro n = 0
		inteiro par = 0
		inteiro impar = 0

		enquanto(n < 10)
		{
			escreva("Digite um número: ")
			leia(n)

			se(n % 2 == 0)
			{
				par++
			}
			senao
			{
				impar++
			}
		}
		escreva("São ", par, " número(s) par(es) e ", impar," número(s) impar(es).")
	}
}
